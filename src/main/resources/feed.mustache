<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <title>{{title}}</title>
    <link>{{link}}</link>
    <description>{{description}}</description>

    {{#items}}
    <item>
      <title>{{title}}</title>
      <link>{{link}}</link>
      <description>{{description}}</description>
    </item>
    {{/items}}
  </channel>
</rss>